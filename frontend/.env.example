# Environment configuration template for frontend

# API Endpoints
REACT_APP_API_URL=http://localhost:3001
REACT_APP_AI_PIPELINE_URL=http://localhost:8000

# Application Settings
REACT_APP_APP_NAME=Personal Brand DNA System
REACT_APP_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_VOICE_ANALYSIS=true
REACT_APP_ENABLE_CONTENT_GENERATION=true
REACT_APP_ENABLE_PAYMENT_PROCESSING=true

# External Services (Replace with your actual keys)
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here
REACT_APP_GOOGLE_ANALYTICS_ID=GA_MEASUREMENT_ID

# Audio Recording Settings
REACT_APP_MAX_RECORDING_DURATION=300
REACT_APP_AUDIO_SAMPLE_RATE=16000
REACT_APP_AUDIO_CHANNELS=1

# Content Generation Settings
REACT_APP_MAX_CONTENT_LENGTH=3000
REACT_APP_DEFAULT_CONTENT_TYPE=post

# UI Configuration
REACT_APP_SHOW_DEBUG_INFO=true
REACT_APP_API_TIMEOUT=30000

# Production Settings (set these for production deployment)
# REACT_APP_API_URL=https://api.yourdomain.com
# REACT_APP_AI_PIPELINE_URL=https://ai.yourdomain.com
# REACT_APP_ENVIRONMENT=production
# REACT_APP_ENABLE_ANALYTICS=true
# REACT_APP_SHOW_DEBUG_INFO=false