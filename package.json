    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.47.0",
    "react-i18next": "^15.6.0",
    "react-icons": "^5.5.0",
    "react-phone-number-input": "^3.4.12",
