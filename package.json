    "socket.io-client": "^4.8.1",
    "typescript": "^4.9.5",
    "vercel": "^32.0.1",
    "web-vitals": "^3.5.0"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@craco/craco": "^7.1.0",
    "@tailwindcss/forms": "^0.5.6",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.8",
    "assert": "^2.1.0",
    "autoprefixer": "^10.4.16",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "compression-webpack-plugin": "^10.0.0",
    "crypto-browserify": "^3.12.1",
    "cssnano": "^6.0.1",
    "cypress": "^13.15.0",
    "fast-glob": "^3.3.3",
    "glob": "^11.0.3",
    "https-browserify": "^1.0.0",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.31",
    "process": "^0.11.10",
    "source-map-explorer": "^2.5.3",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "tailwindcss": "^3.3.5",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "webpack-bundle-analyzer": "^4.9.1",
    "workbox-webpack-plugin": "^7.0.0"
  },
  "scripts": {
    "start": "npm run start:web",
    "build": "npm run build:all",
    "start:web": "craco start",
    "start:agents": "cd apps/agents && npm run start",
    "build:web": "NODE_OPTIONS='--max-old-space-size=4096' DISABLE_ESLINT_PLUGIN=true TSC_COMPILE_ON_ERROR=true GENERATE_SOURCEMAP=false CI=false craco build",
    "build:agents": "npm run build:shared && npm run build:queue && cd apps/agents && npm run build",
    "build:shared": "cd packages/shared && npm run build",
    "build:queue": "cd packages/queue && npm run build",
    "build:all": "npm run build:shared && npm run build:queue && npm run build:web && npm run build:agents",
    "build:analyze": "ANALYZE=true npm run build:web",
    "vercel-build": "npm run build",
    "test": "craco test",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:headless": "cypress run --headless",
    "test:e2e:chrome": "cypress run --browser chrome",
    "test:e2e:firefox": "cypress run --browser firefox",
    "test:all": "npm run test -- --coverage --passWithNoTests && npm run test:e2e",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "analyze:bundle": "webpack-bundle-analyzer build/bundle-stats.json",
    "analyze:size": "npm run build && npm run analyze",
    "optimize:images": "node scripts/optimize-images.js",
    "optimize:imports": "node scripts/optimize-imports.js",
    "optimize:bundle": "node scripts/optimize-bundle.js",
    "setup:agents": "node scripts/setup-agent-cloud-services.js",
    "setup:agents:auto": "node scripts/automate-cloud-setup.js",
    "setup:agents:local": "node scripts/automate-cloud-setup.js local",
    "setup:agents:prod": "node scripts/automate-cloud-setup.js production",
    "deploy:agents": "bash scripts/deploy-agents.sh",
    "agents:dev": "cd apps/agents && npm run dev",
    "agents:logs": "docker-compose -f docker-compose.agents.yml logs -f",
    "start:local-agents": "bash scripts/start-local-agents.sh"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:3001"
}
