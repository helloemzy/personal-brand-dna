    "dompurify": "^3.2.6",
    "dotenv": "^16.3.1",
    "framer-motion": "^12.23.0",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.516.0",
    "moment": "^2.30.1",
    "nodemailer": "^6.9.7",
    "pdfmake": "^0.2.20",
    "react": "^18.2.0",
    "react-big-calendar": "^1.19.4",
    "react-dnd": "^16.0.1",
